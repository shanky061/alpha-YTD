{
  "name": "Alpha Youtube Downloader",
  "version": "1.0",
  "description": "Download Youtube Videos, Simple Way.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action" :
  {
    "default_icon" : "icon-19.png",
    "default_title" : "Download Video",
